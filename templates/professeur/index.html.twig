{% extends 'base.html.twig' %}

{% block title %}Zone Enseignant!{% endblock %}

{% block body %}

{# {{ dump(ressource) }} #}
<div id="bienvenue">
  {# <p> Bienvenue {{ enseignant.nom }}</p> #}
</div>

<div id="ressource" class="col-md-6 mt-5 d-none">
<table class="table table-bordered col-md-8 mx-auto">
{% for row in ressource %}
<tr>
    <td>{{ row.type }}</td>
    <td>{{ row.Titre }}</td>
    <td>{{ row.Description }}</td>
    <td>{{ row.link }}</td>
    <td>{{ row.createAt|date("d/m/Y") }}</td>
   
  </tr>
</div>

    
{% endfor %}
</table>
{% endblock %}
